doctype html
html
  head
    link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
    script(src='/javascripts/vendors/jquery.min.js')
    script(src='/javascripts/vendors/bootstrap.min.js')
    script(src='/javascripts/vendors/popper.min.js')
    script(src='/javascripts/vendors/jquery-1.9.1.js')
    script(src='/javascripts/vendors/jquery-ui.js') 
    script(src='/javascripts/vendors/moment.min.js')  
    script(src='/javascripts/vendors/bootstrap-datetimepicker.min.js')
    link(rel='stylesheet', href='/stylesheets/bootstrap-datetimepicker.min.css')
    script(src='/javascripts/show.js')
    link(rel='stylesheet', href='/stylesheets/test.css')
    link(rel='stylesheet', href='/stylesheets/table.css')
  body
    .row
      .col-sm-12.nav.navbar.navbar-inverse.navbar-fixed-top
        .container
          .navbar-header
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='#') Project name
          #navbar.collapse.navbar-collapse
            ul.nav.navbar-nav
              li.active
                a(href='#') Home
              li
                a(href='#table') Reports
              li
                a(href='#contact') Teams
            ul.nav.navbar-nav.navbar-right
              if !user
                li
                  a(href='/users/login')
                    span.glyphicon.glyphicon-log-in 
                    |  Login
                li
                  a(href='#')
                    span.glyphicon.glyphicon-cog
                    |  Settings
              else
                li
                  a(href='/users/logout')
                    span.glyphicon.glyphicon-log-out 
                    |  Logout
                li
                  a(href='#')
                    span.glyphicon.glyphicon-cog
                    |  Settings
    block content

  

    